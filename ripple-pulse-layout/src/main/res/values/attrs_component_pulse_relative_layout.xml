<resources>Ripple
    <declare-styleable name="RipplePulseRelativeLayout">
        <attr name="showPreview" format="boolean" />
        <attr name="rippleColor" format="color|reference" />
        <attr name="pulseType" format="enum">
            <enum name="fill" value="0"/>
            <enum name="stroke" value="1"/>
        </attr>
        <attr name="pulseDuration" format="integer|reference"/>
        <attr name="startDelay" format="integer|reference"/>
        <attr name="endDelay" format="integer|reference"/>
        <attr name="rippleStrokeWidth" format="dimension|reference"/>
        <attr name="rippleStartRadiusPercent" format="float"/>
        <attr name="rippleEndRadiusPercent" format="float"/>
        <attr name="pulseInterpolator" format="reference"/>
    </declare-styleable>
</resources>
